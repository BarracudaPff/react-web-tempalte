{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, AntD, Vite (with HMR), Sass/Scss and ofc Typescript <3",
  "keywords": [
    "react",
    "ant",
    "design",
    "vite",
    "typescript",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Viktor Persson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BarracudaPff/react-web-tempalte.git"
  },
  "bugs": {
    "url": "https://github.com/BarracudaPff/react-web-tempalte/issues"
  },
  "homepage": "https://github.com/BarracudaPff/react-web-tempalte",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "start:dev": "vite",
    "start:prod": "vite --mode prod",
    "build": "vite build --mode prod",
    "build:server": "vite build --mode prod ; cd build ; python3 -m http.server 8001",
    "serve": "vite preview",
    "lint": "eslint --cache --ext .js,.jsx,.tsx,.ts ./src",
    "lint:quiet": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "test:unit": "vite-jest --no-cache --passWithNoTests"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/redux-logger": "^3.0.9",
    "@vitejs/plugin-react": "^1.3.2",
    "browserify": "^17.0.0",
    "jest": "^27.4.5",
    "jest-environment-jsdom": "^28.1.0",
    "less": "^4.1.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.51.0",
    "ts-loader": "^9.2.6",
    "type-fest": "^2.11.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.8",
    "vite-jest": "^0.1.4",
    "vite-plugin-fonts": "^0.4.0",
    "vite-plugin-imp": "^2.1.8"
  },
  "dependencies": {
    "@barracudapff/async-storage": "git+https://github.com/BarracudaPff/async-storage#4e633d128e18669c25f44ea34fb1fd4e5a6afceb",
    "antd": "^4.20.2",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-token-auth": "^2.3.8",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "serialize-query-params": "^1.3.6",
    "ts-custom-error": "^3.2.0",
    "typesafe-actions": "^5.1.0"
  }
}
