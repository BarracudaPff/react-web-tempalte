{
  "name": "premiertips-front-r",
  "version": "0.1.0",
  "description": "Premier tips front app with React, AntD, Vite (with HMR), Sass/Scss and ofc Typescript <3",
  "keywords": [
    "react",
    "ant",
    "design",
    "vite",
    "typescript",
    "sass",
    "hmr"
  ],
  "author": "Kirill Krylov <mrm.kikll@gmail.com> https://github.com/BarracudaPff",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BarracudaPff/premiertips-front-r.git"
  },
  "bugs": {
    "url": "https://github.com/BarracudaPff/premiertips-front-r/issues"
  },
  "homepage": "https://github.com/BarracudaPff/premiertips-front-r",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "start:dev": "vite",
    "start:prod": "vite --mode prod",
    "build": "vite build --mode prod",
    "build:server": "vite build --mode prod ; cd build ; python3 -m http.server 8001",
    "serve": "vite preview",
    "lint": "eslint --cache --ext .js,.jsx,.tsx,.ts ./src",
    "lint:quiet": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "test": "jest --coverage --detectOpenHandles --no-cache"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/redux-logger": "^3.0.9",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-jest": "^28.1.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "browserify": "^17.0.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^27.4.5",
    "less": "^4.1.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.51.0",
    "type-fest": "^2.11.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.8",
    "vite-jest": "^0.1.4",
    "vite-plugin-fonts": "^0.4.0",
    "vite-plugin-imp": "^2.1.8",
    "vite-plugin-svgr": "^2.1.0"
  },
  "dependencies": {
    "@barracudapff/async-storage": "git+https://github.com/BarracudaPff/async-storage#4e633d128e18669c25f44ea34fb1fd4e5a6afceb",
